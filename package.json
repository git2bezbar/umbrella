{
  "name": "@fork2e/umbrella",
  "description": "☂️ Umbrella UI React components based on @shadcn-ui",
  "version": "0.0.4",
  "type": "module",
  "main": "dist/lib.cjs.js",
  "module": "dist/lib/esm/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/git2bezbar/umbrella.git"
  },
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf ./dist || true",
    "build": "npm run clean && rollup -c && npm run build:types && npm run build:styles",
    "build:types": "tsc --emitDeclarationOnly",
    "build:styles": "postcss ./src/index.css -o ./dist/lib.min.css",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
